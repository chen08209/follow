{
  "private": true,
  "packageManager": "pnpm@7.9.3",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "dev": "pnpm -C play dev",
    "dev:docs": "pnpm -C docs dev",
    "build": "pnpm run -C internal/build start",
    "build:theme": "pnpm -C packages/theme-chalk build",
    "build:docs": "pnpm -C docs build",
    "deploy": "bash deploy.sh",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "lint:fix": "eslint --fix . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json",
    "clean:dist": "rimraf dist",
    "format": "prettier --write .",
    "type-check": "vue-tsc --noEmit",
    "stub": "pnpm run -r --parallel stub",
    "postinstall": "pnpm stub"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.1",
    "@element-plus/icons-vue": "^2.0.10",
    "@follow-ui/components": "workspace:*",
    "@follow-ui/constants": "workspace:*",
    "@follow-ui/directives": "workspace:*",
    "@follow-ui/hooks": "workspace:*",
    "@follow-ui/locale": "workspace:*",
    "@follow-ui/theme-chalk": "workspace:*",
    "@follow-ui/tokens": "workspace:*",
    "@follow-ui/utils": "workspace:*",
    "@popperjs/core": "npm:@sxzz/popperjs-es@^2.11.7",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.186",
    "@types/lodash-es": "^4.17.6",
    "@vue/runtime-core": "^3.2.40",
    "@vueuse/core": "^9.3.0",
    "async-validator": "^4.2.5",
    "dayjs": "^1.11.5",
    "eslint-define-config": "^1.7.0",
    "follow-ui": "workspace:*",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.2",
    "pretty-quick": "^3.1.3"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "^2.4.0",
    "@follow-ui/build-constants": "workspace:*",
    "@follow-ui/build-utils": "workspace:*",
    "@follow-ui/eslint-config": "workspace:*",
    "@pnpm/types": "^8.7.0",
    "@types/gulp": "^4.0.9",
    "@types/node": "^16.11.64",
    "@types/sass": "^1.43.1",
    "@vue/tsconfig": "^0.1.3",
    "chalk": "^5.1.0",
    "components-helper": "^2.1.3",
    "consola": "^2.15.3",
    "eslint": "8.22.0",
    "fast-glob": "^3.2.12",
    "gulp": "^4.0.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "typescript": "~4.7.4",
    "unplugin-vue-macros": "^0.13.3",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5",
    "vue-tsc": "^0.39.5"
  }
}
